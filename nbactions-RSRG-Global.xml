<?xml version="1.0" encoding="UTF-8"?>
<actions>
        <action>
            <actionName>CUSTOM-format</actionName>
            <displayName>format</displayName>
            <goals>
                <goal>java-formatter:format</goal>
            </goals>
        </action>
        <action>
            <actionName>CUSTOM-rebase</actionName>
            <displayName>rebase</displayName>
            <goals>
                <goal>edu.clemson.cs.resolve:git-plugin:rebase</goal>
            </goals>
            <properties>
                <repoLocation>git://github.com/ClemsonRSRG/RESOLVE.git</repoLocation>
                <branch>master</branch>
            </properties>
        </action>
        <action>
            <actionName>CUSTOM-rebase --continue</actionName>
            <displayName>rebase --continue</displayName>
            <goals>
                <goal>edu.clemson.cs.resolve:git-plugin:rebase-continue</goal>
            </goals>
        </action>
        <action>
            <actionName>CUSTOM-rebase --abort</actionName>
            <displayName>rebase --abort</displayName>
            <goals>
                <goal>edu.clemson.cs.resolve:git-plugin:rebase-abort</goal>
            </goals>
        </action>
        <action>
            <actionName>CUSTOM-rebase --skip</actionName>
            <displayName>rebase --skip</displayName>
            <goals>
                <goal>edu.clemson.cs.resolve:git-plugin:rebase-skip</goal>
            </goals>
        </action>
        <action>
            <actionName>run</actionName>
            <goals>
                <goal>process-classes</goal>
                <goal>org.codehaus.mojo:exec-maven-plugin:1.2.1:exec</goal>
            </goals>
            <properties>
                <exec.args>-classpath %classpath edu.clemson.cs.r2jt.Main -vcs Facilities\User\Int_Max_Example_Facility.fa</exec.args>
                <exec.executable>java</exec.executable>
                <exec.workingdir>C:\Users\Blair\Development\RESOLVE-Workspace\RESOLVE\Main</exec.workingdir>
            </properties>
        </action>
        <action>
            <actionName>debug</actionName>
            <goals>
                <goal>process-classes</goal>
                <goal>org.codehaus.mojo:exec-maven-plugin:1.2.1:exec</goal>
            </goals>
            <properties>
                <exec.args>-Xdebug -Xrunjdwp:transport=dt_socket,server=n,address=${jpda.address} -classpath %classpath edu.clemson.cs.r2jt.Main -vcs Facilities\User\Int_Max_Example_Facility.fa</exec.args>
                <exec.executable>java</exec.executable>
                <jpda.listen>true</jpda.listen>
                <exec.workingdir>C:\Users\Blair\Development\RESOLVE-Workspace\RESOLVE\Main</exec.workingdir>
            </properties>
        </action>
        <action>
            <actionName>profile</actionName>
            <goals>
                <goal>process-classes</goal>
                <goal>org.codehaus.mojo:exec-maven-plugin:1.2.1:exec</goal>
            </goals>
            <properties>
                <exec.args>${profiler.args} -classpath %classpath edu.clemson.cs.r2jt.Main -vcs Facilities\User\Int_Max_Example_Facility.fa</exec.args>
                <exec.executable>${profiler.java}</exec.executable>
                <exec.workingdir>C:\Users\Blair\Development\RESOLVE-Workspace\RESOLVE\Main</exec.workingdir>
            </properties>
        </action>
    </actions>
